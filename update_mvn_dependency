[opc@sr-sandbox-jumpserver helidon-alltitude-demo]$ for i in ojdbc8 ucp osdt_core osdt_cert oraclepki javax.json-1.0.4 json-simple-1.1 orajsoda-1.1.4 org.apache.commons.io xmlparserv2; do echo mvn -X install:install-file " -DgroupId=com.oracle.jdbc" "-DartifactId=$i" "-Dversion=19.3.0" "-Dpackaging=jar" "-Dfile=.m2\\\$i.jar"; done;

